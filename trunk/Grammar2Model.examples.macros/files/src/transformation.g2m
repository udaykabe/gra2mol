rule 'mapScript'
	from file f
	to   Script
	queries
		vElem : /f//#version_statement;
		sElem : /f//#statement;
	mappings
		version      = vElem;
		instructions = sElem;
end_rule

rule 'mapVersion'
	from version_statement v
	to   Version
	queries
		vElem : /v//#version_attr;
	mappings
		build = vElem.DQVAL;
end_rule

rule 'mapTagStatement'
	from statement/tag_statement f
	to   Tag
	queries
		
	mappings
		
end_rule


